      "program": "${workspaceFolder}/CrossWord/bin/Debug/net7.0/CrossWord.dll",
      "args": [
        "-i",
        "${workspaceFolder}/templates/template1.txt",
        "-o",
        "out.txt",
        "-d",
        "${workspaceFolder}/dict/en"
